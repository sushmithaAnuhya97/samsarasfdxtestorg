<?xml version="1.0" encoding="UTF-8"?>
<ApprovalProcess xmlns="http://soap.sforce.com/2006/04/metadata">
    <active>true</active>
    <allowRecall>true</allowRecall>
    <allowedSubmitters>
        <submitter>User_Role_Group_For_Free_Trial</submitter>
        <type>group</type>
    </allowedSubmitters>
    <allowedSubmitters>
        <type>owner</type>
    </allowedSubmitters>
    <allowedSubmitters>
        <submitter>Enterprise_UK_AE</submitter>
        <type>role</type>
    </allowedSubmitters>
    <allowedSubmitters>
        <submitter>Major_Team_1_AE</submitter>
        <type>role</type>
    </allowedSubmitters>
    <allowedSubmitters>
        <submitter>Major_Team_2_AE</submitter>
        <type>role</type>
    </allowedSubmitters>
    <allowedSubmitters>
        <submitter>Major_Team_3_AE</submitter>
        <type>role</type>
    </allowedSubmitters>
    <allowedSubmitters>
        <submitter>Operations</submitter>
        <type>role</type>
    </allowedSubmitters>
    <allowedSubmitters>
        <submitter>SEL_Fleet_ENT_AE_EMEA_UK_Team_1</submitter>
        <type>role</type>
    </allowedSubmitters>
    <allowedSubmitters>
        <submitter>SEL_Fleet_ENT_AE_NA_US_Team_1</submitter>
        <type>role</type>
    </allowedSubmitters>
    <allowedSubmitters>
        <submitter>SEL_Fleet_ENT_AE_NA_US_Team_2</submitter>
        <type>role</type>
    </allowedSubmitters>
    <allowedSubmitters>
        <submitter>SEL_Fleet_ENT_AE_NA_US_Team_3</submitter>
        <type>role</type>
    </allowedSubmitters>
    <allowedSubmitters>
        <submitter>SEL_Fleet_ENT_AE_SLED_NA_US_Team_3</submitter>
        <type>role</type>
    </allowedSubmitters>
    <allowedSubmitters>
        <submitter>SEL_Fleet_ENT_Director_2_NA</submitter>
        <type>role</type>
    </allowedSubmitters>
    <allowedSubmitters>
        <submitter>SEL_Fleet_ENT_Director_3_NA</submitter>
        <type>role</type>
    </allowedSubmitters>
    <allowedSubmitters>
        <submitter>SEL_Fleet_ENT_NA_Team_2_Regional</submitter>
        <type>role</type>
    </allowedSubmitters>
    <allowedSubmitters>
        <submitter>SEL_Fleet_ENT_VP</submitter>
        <type>role</type>
    </allowedSubmitters>
    <allowedSubmitters>
        <submitter>STR_Fleet_ENT_Director_1_NA</submitter>
        <type>role</type>
    </allowedSubmitters>
    <approvalPageFields>
        <field>Name</field>
        <field>Owner</field>
        <field>Trial_Days_Open__c</field>
        <field>Reason_for_Extension__c</field>
    </approvalPageFields>
    <approvalStep>
        <allowDelegate>true</allowDelegate>
        <assignedApprover>
            <approver>
                <type>userHierarchyField</type>
            </approver>
        </assignedApprover>
        <entryCriteria>
            <booleanFilter>(1 OR 2 OR 4 OR 5) AND 3</booleanFilter>
            <criteriaItems>
                <field>User.UserRole</field>
                <operation>equals</operation>
                <value>Enterprise - Director - 1,Enterprise - Team 1 - AE,Major - Team 1 - AE,Enterprise - Director - 2,Enterprise - Team 2 - AE,Major - Team 2 - AE</value>
            </criteriaItems>
            <criteriaItems>
                <field>User.UserRole</field>
                <operation>equals</operation>
                <value>Enterprise - Director – 3,Enterprise - Team 3 – AE,Major - Team 3 - AE,Enterprise - AE - UK,Major Accounts – AE,Fleet ENT AE EMEA UK,SEL Fleet ENT AE EMEA FR,SEL Fleet ENT AE EMEA DACH</value>
            </criteriaItems>
            <criteriaItems>
                <field>Opportunity.Trial_Period__c</field>
                <operation>greaterOrEqual</operation>
                <value>60</value>
            </criteriaItems>
            <criteriaItems>
                <field>User.UserRole</field>
                <operation>equals</operation>
                <value>SEL Fleet ENT AE SLED NA US Team 3</value>
            </criteriaItems>
            <criteriaItems>
                <field>User.UserRole</field>
                <operation>equals</operation>
                <value>SEL Fleet ENT AE EMEA UK Team 2</value>
            </criteriaItems>
        </entryCriteria>
        <ifCriteriaNotMet>ApproveRecord</ifCriteriaNotMet>
        <label>Enterprise Free Trial Approval - Manager Approval</label>
        <name>Manager_Approval</name>
    </approvalStep>
    <description>Free trial approval process for the Enterprise team</description>
    <emailTemplate>Finance/Free_Trial_Request</emailTemplate>
    <enableMobileDeviceAccess>true</enableMobileDeviceAccess>
    <entryCriteria>
        <formula>AND(ISPICKVAL(Type , &apos;Free Trial Opportunity&apos;))</formula>
    </entryCriteria>
    <finalApprovalActions>
        <action>
            <name>Approved_Free_Trial_Status_Update</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>Reset_Free_Trial_Approval</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>Update_Trial_End_Date_for_Ent_Approval</name>
            <type>FieldUpdate</type>
        </action>
    </finalApprovalActions>
    <finalApprovalRecordLock>false</finalApprovalRecordLock>
    <finalRejectionActions>
        <action>
            <name>Rejected_Free_Trial_Status_Update</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>Reset_Free_Trial_Approval</name>
            <type>FieldUpdate</type>
        </action>
    </finalRejectionActions>
    <finalRejectionRecordLock>false</finalRejectionRecordLock>
    <initialSubmissionActions>
        <action>
            <name>Pending_Free_Trial_Status_Update</name>
            <type>FieldUpdate</type>
        </action>
    </initialSubmissionActions>
    <label>Enterprise Free Trial Approval</label>
    <nextAutomatedApprover>
        <useApproverFieldOfRecordOwner>true</useApproverFieldOfRecordOwner>
        <userHierarchyField>Manager</userHierarchyField>
    </nextAutomatedApprover>
    <processOrder>9</processOrder>
    <recallActions>
        <action>
            <name>Recalled_Free_Trial_Status_Update</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>Reset_Free_Trial_Approval</name>
            <type>FieldUpdate</type>
        </action>
    </recallActions>
    <recordEditability>AdminOrCurrentApprover</recordEditability>
    <showApprovalHistory>true</showApprovalHistory>
</ApprovalProcess>
