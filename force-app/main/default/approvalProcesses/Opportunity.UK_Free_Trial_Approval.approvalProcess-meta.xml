<?xml version="1.0" encoding="UTF-8"?>
<ApprovalProcess xmlns="http://soap.sforce.com/2006/04/metadata">
    <active>true</active>
    <allowRecall>true</allowRecall>
    <allowedSubmitters>
        <type>owner</type>
    </allowedSubmitters>
    <allowedSubmitters>
        <submitter>Fleet_IS_EMEA_NL_Manager</submitter>
        <type>role</type>
    </allowedSubmitters>
    <allowedSubmitters>
        <submitter>Fleet_IS_EMEA_UK_Team_3_Manager</submitter>
        <type>role</type>
    </allowedSubmitters>
    <allowedSubmitters>
        <submitter>Management</submitter>
        <type>role</type>
    </allowedSubmitters>
    <allowedSubmitters>
        <submitter>MM_Fleet_IS_AE2_EMEA_DE_Team_4</submitter>
        <type>role</type>
    </allowedSubmitters>
    <allowedSubmitters>
        <submitter>MM_Fleet_IS_AE2_EMEA_ES_Team_4</submitter>
        <type>role</type>
    </allowedSubmitters>
    <allowedSubmitters>
        <submitter>MM_Fleet_IS_AE2_EMEA_FR_Manager_Team_5</submitter>
        <type>role</type>
    </allowedSubmitters>
    <allowedSubmitters>
        <submitter>MM_Fleet_IS_AE2_EMEA_FR_Team_5</submitter>
        <type>role</type>
    </allowedSubmitters>
    <allowedSubmitters>
        <submitter>MM_Fleet_IS_AE2_EMEA_IT</submitter>
        <type>role</type>
    </allowedSubmitters>
    <allowedSubmitters>
        <submitter>MM_Fleet_IS_AE2_EMEA_NL_Team_6</submitter>
        <type>role</type>
    </allowedSubmitters>
    <allowedSubmitters>
        <submitter>MM_Fleet_IS_AE2_EMEA_UK_Team_1</submitter>
        <type>role</type>
    </allowedSubmitters>
    <allowedSubmitters>
        <submitter>MM_Fleet_IS_AE2_EMEA_UK_Team_1_Manager</submitter>
        <type>role</type>
    </allowedSubmitters>
    <allowedSubmitters>
        <submitter>MM_Fleet_IS_AE2_EMEA_UK_Team_2</submitter>
        <type>role</type>
    </allowedSubmitters>
    <allowedSubmitters>
        <submitter>MM_Fleet_IS_AE2_EMEA_UK_Team_3</submitter>
        <type>role</type>
    </allowedSubmitters>
    <allowedSubmitters>
        <submitter>MM_Fleet_IS_EMEA_DE_Manager</submitter>
        <type>role</type>
    </allowedSubmitters>
    <allowedSubmitters>
        <submitter>MM_Fleet_IS_EMEA_UK_Team_2_Manager</submitter>
        <type>role</type>
    </allowedSubmitters>
    <allowedSubmitters>
        <submitter>Operations</submitter>
        <type>role</type>
    </allowedSubmitters>
    <approvalPageFields>
        <field>Name</field>
        <field>Owner</field>
        <field>Trial_Days_Open__c</field>
        <field>Reason_for_Extension__c</field>
    </approvalPageFields>
    <approvalStep>
        <allowDelegate>true</allowDelegate>
        <assignedApprover>
            <approver>
                <type>userHierarchyField</type>
            </approver>
        </assignedApprover>
        <label>UK Free Trial Approval - Director Approval</label>
        <name>Director_Approval_Request_for_Free_Trial_Extension</name>
    </approvalStep>
    <description>Free trial approval process for the UK MidMarket team</description>
    <emailTemplate>Finance/Free_Trial_Request</emailTemplate>
    <enableMobileDeviceAccess>true</enableMobileDeviceAccess>
    <entryCriteria>
        <formula>AND(ISPICKVAL(Type , &apos;Free Trial Opportunity&apos;), ($User.Enterprise_Major_Account_Team__c = false), ( $User.Industrial_Team__c = false), 
ISPICKVAL($User.Entity__c, &quot;Samsara UK&quot;))</formula>
    </entryCriteria>
    <finalApprovalActions>
        <action>
            <name>Approved_Free_Trial_Status_Update</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>Reset_Free_Trial_Approval</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>Update_Trial_End_Date_for_MM_Approval</name>
            <type>FieldUpdate</type>
        </action>
    </finalApprovalActions>
    <finalApprovalRecordLock>false</finalApprovalRecordLock>
    <finalRejectionActions>
        <action>
            <name>Rejected_Free_Trial_Status_Update</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>Reset_Free_Trial_Approval</name>
            <type>FieldUpdate</type>
        </action>
    </finalRejectionActions>
    <finalRejectionRecordLock>false</finalRejectionRecordLock>
    <initialSubmissionActions>
        <action>
            <name>Pending_Free_Trial_Status_Update</name>
            <type>FieldUpdate</type>
        </action>
    </initialSubmissionActions>
    <label>UK Free Trial Approval</label>
    <nextAutomatedApprover>
        <useApproverFieldOfRecordOwner>false</useApproverFieldOfRecordOwner>
        <userHierarchyField>Manager</userHierarchyField>
    </nextAutomatedApprover>
    <processOrder>3</processOrder>
    <recallActions>
        <action>
            <name>Recalled_Free_Trial_Status_Update</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>Reset_Free_Trial_Approval</name>
            <type>FieldUpdate</type>
        </action>
    </recallActions>
    <recordEditability>AdminOrCurrentApprover</recordEditability>
    <showApprovalHistory>true</showApprovalHistory>
</ApprovalProcess>
