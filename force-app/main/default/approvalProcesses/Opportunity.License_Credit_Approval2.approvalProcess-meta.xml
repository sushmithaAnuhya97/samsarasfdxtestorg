    <entryCriteria>
        <formula>AND(
ISPICKVAL(Type , &apos;Rebate&apos;),
ISPICKVAL( Rebate_Type__c , &apos;License Term Credit&apos;),
OR(CONTAINS(Owner.UserRole.Name, &apos;CS&apos;),CONTAINS(Owner.UserRole.Name,&apos;SE&apos;))
)</formula>
    </entryCriteria>
