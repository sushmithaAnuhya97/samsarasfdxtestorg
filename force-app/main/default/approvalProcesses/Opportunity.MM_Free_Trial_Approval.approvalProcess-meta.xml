    </approvalPageFields>
    <approvalStep>
        <allowDelegate>true</allowDelegate>
        <assignedApprover>
            <approver>
                <type>userHierarchyField</type>
            </approver>
        </assignedApprover>
        <label>MM Free Trial Approval	- Manager Approval</label>
        <name>Manager_Approval</name>
    </approvalStep>
    <approvalStep>
        <allowDelegate>true</allowDelegate>
        <assignedApprover>
            <approver>
                <type>userHierarchyField</type>
            </approver>
        </assignedApprover>
        <entryCriteria>
            <criteriaItems>
                <field>Opportunity.Trial_Period__c</field>
                <operation>greaterOrEqual</operation>
                <value>60</value>
            </criteriaItems>
        </entryCriteria>
        <label>MM Free Trial Approval	- AVP Approval</label>
        <name>AVP_Approval_Request_for_Free_Trial_Extension</name>
        <rejectBehavior>
            <type>RejectRequest</type>
        </rejectBehavior>
    </approvalStep>
    <description>Free trial approval process for the MidMarket team</description>
    <emailTemplate>Finance/Free_Trial_Request</emailTemplate>
    <enableMobileDeviceAccess>true</enableMobileDeviceAccess>
    <entryCriteria>
        <formula>AND(ISPICKVAL(Type , &apos;Free Trial Opportunity&apos;), ($User.Enterprise_Major_Account_Team__c = false), ( $User.Industrial_Team__c = false), CONTAINS($UserRole.Name, &apos;Fleet IS&apos;)
)</formula>
    </entryCriteria>
    <finalApprovalActions>
