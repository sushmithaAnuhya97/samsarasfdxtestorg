    </approvalPageFields>
    <approvalStep>
        <allowDelegate>true</allowDelegate>
        <assignedApprover>
            <approver>
                <type>userHierarchyField</type>
            </approver>
        </assignedApprover>
        <description>Manager Approval Needed</description>
        <label>Promo Discount Approval - Manager Approval</label>
        <name>Manager_Approval_Request</name>
    </approvalStep>
    <approvalStep>
        <allowDelegate>true</allowDelegate>
        <assignedApprover>
            <approver>
                <type>userHierarchyField</type>
            </approver>
        </assignedApprover>
        <description>AVP Approval Needed</description>
        <entryCriteria>
            <criteriaItems>
                <field>Opportunity.Blended_Discretionary_Discount__c</field>
                <operation>greaterThan</operation>
                <value>15.0</value>
            </criteriaItems>
        </entryCriteria>
        <label>Promo Discount Approval - AVP Approval</label>
        <name>AVP_Approval_Request</name>
        <rejectBehavior>
            <type>RejectRequest</type>
        </rejectBehavior>
    </approvalStep>
    <description>Discount approval for promo opportunities</description>
    <emailTemplate>unfiled$public/Discount_Approval_Notification_to_Managers</emailTemplate>
    <enableMobileDeviceAccess>true</enableMobileDeviceAccess>
    <entryCriteria>
        <formula>AND( 
ISPICKVAL( Type , &apos;Promo Opportunity&apos;) , Blended_Discretionary_Discount__c  &gt;  Max_Approved_Discount__c, 
OR(
$User.ProfileId &lt;&gt; &apos;00e1a000000UoNc&apos;,
$User.ProfileId &lt;&gt; &apos;00e1a000000i8yf&apos;)
)</formula>
    </entryCriteria>
