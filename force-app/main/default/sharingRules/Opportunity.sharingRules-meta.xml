    <sharingCriteriaRules>
        <fullName>Contract_Finance_Rebate_Oppty_Access</fullName>
        <accessLevel>Edit</accessLevel>
        <description>Sharing rule enabled to allow Rebate Oppty editability</description>
        <label>[Contract Finance] Rebate Oppty Access</label>
        <sharedTo>
            <role>Contract_Finance</role>
        </sharedTo>
        <booleanFilter>1 AND (2 OR 3)</booleanFilter>
        <criteriaItems>
            <field>Type</field>
            <operation>equals</operation>
            <value>Rebate</value>
        </criteriaItems>
        <criteriaItems>
            <field>Owner_Role_Copy__c</field>
            <operation>contains</operation>
            <value>AE1</value>
        </criteriaItems>
        <criteriaItems>
            <field>Owner_Role_Copy__c</field>
            <operation>contains</operation>
            <value>SMB</value>
        </criteriaItems>
        <includeRecordsOwnedByAll>false</includeRecordsOwnedByAll>
    </sharingCriteriaRules>
    <sharingCriteriaRules>
        <fullName>Contract_Finance_Revenue_Oppty_Access</fullName>
