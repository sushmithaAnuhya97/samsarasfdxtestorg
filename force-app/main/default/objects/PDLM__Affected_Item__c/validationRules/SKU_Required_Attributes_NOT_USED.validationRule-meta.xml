<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>SKU_Required_Attributes_NOT_USED</fullName>
    <active>false</active>
    <errorConditionFormula>AND(
PDLM__Item_Revision__r.PDLM__Master_Item__r.PDLM__Category__r.Name = &quot;SKU&quot;,
ISNULL(PDLM__Item_Revision__r.Associated_Product_ID__c),
 PDLM__Change_Order_lk__r.PDLM__Status_lk__r.PDLM__In_Approval_Process__c )</errorConditionFormula>
    <errorMessage>Associate Product ID is required on SKU before being released</errorMessage>
</ValidationRule>
