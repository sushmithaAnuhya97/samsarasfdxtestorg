<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>MCO_cannot_move_items_from_Development</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
 TEXT(PDLM__Item_Revision__r.PDLM__Master_Item__r.PDLM__Latest_Released_Revision__r.PDLM__Lifecycle_Phase__c)=&quot;Development&quot;,
 NOT(TEXT(PDLM__Item_Revision__r.PDLM__Lifecycle_Phase__c)= &quot;Development&quot;),
 PDLM__Item_Revision__r.IR_Revision_Count__c &gt;1,
 OR(
 PDLM__Change_Order_lk__r.PDLM__Category__r.Name = &quot;MCO - Cost&quot;,
 PDLM__Change_Order_lk__r.PDLM__Category__r.Name = &quot;MCO - Firmware&quot;,
 PDLM__Change_Order_lk__r.PDLM__Category__r.Name = &quot;MCO - General&quot;
 )
 )</errorConditionFormula>
    <errorMessage>MCOs are not allowed for changing Item lifecycles from Development to Other</errorMessage>
</ValidationRule>
