<?xml version="1.0" encoding="UTF-8"?>
<ListView xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Opportunity_Activity_Level_Review</fullName>
    <booleanFilter>((1 AND 2) OR (3 AND 4) OR (5 AND 6) OR (7 AND 8)) AND 9 AND 10</booleanFilter>
    <columns>OPPORTUNITY.NAME</columns>
    <columns>ACCOUNT.NAME</columns>
    <columns>OPPORTUNITY.LAST_STAGE_CHANGE</columns>
    <columns>Opportunity_Age__c</columns>
    <columns>OPPORTUNITY.STAGE_NAME</columns>
    <columns>OPPORTUNITY.PROBABILITY</columns>
    <columns>OPPORTUNITY.CLOSE_DATE</columns>
    <columns>OPPORTUNITY.AMOUNT</columns>
    <columns>CORE.USERS.ALIAS</columns>
    <filterScope>Everything</filterScope>
    <filters>
        <field>OPPORTUNITY.STAGE_NAME</field>
        <operation>equals</operation>
        <value>Qualified</value>
    </filters>
    <filters>
        <field>Opportunity_Age__c</field>
        <operation>greaterThan</operation>
        <value>120</value>
    </filters>
    <filters>
        <field>OPPORTUNITY.STAGE_NAME</field>
        <operation>equals</operation>
        <value>Trial,Proposal,Decision</value>
    </filters>
    <filters>
        <field>OPPORTUNITY.LAST_STAGE_CHANGE</field>
        <operation>notEqual</operation>
        <value>,LAST_N_DAYS:80</value>
    </filters>
    <filters>
        <field>OPPORTUNITY.STAGE_NAME</field>
        <operation>equals</operation>
        <value>Purchase</value>
    </filters>
    <filters>
        <field>OPPORTUNITY.LAST_STAGE_CHANGE</field>
        <operation>notEqual</operation>
        <value>,LAST_N_DAYS:40</value>
    </filters>
    <filters>
        <field>OPPORTUNITY.STAGE_NAME</field>
        <operation>equals</operation>
        <value>Closing,Orders Processing,Ready to Ship</value>
    </filters>
    <filters>
        <field>OPPORTUNITY.LAST_STAGE_CHANGE</field>
        <operation>notEqual</operation>
        <value>,LAST_N_DAYS:20</value>
    </filters>
    <filters>
        <field>OPPORTUNITY.CLOSE_DATE</field>
        <operation>equals</operation>
        <value>THIS_FISCAL_QUARTER,NEXT_FISCAL_QUARTER</value>
    </filters>
    <filters>
        <field>OPPORTUNITY.RECORDTYPE</field>
        <operation>equals</operation>
        <value>Opportunity.Standard_Opportunity</value>
    </filters>
    <label>Opportunity Activity Level Review</label>
    <sharedTo>
        <allInternalUsers></allInternalUsers>
    </sharedTo>
</ListView>
