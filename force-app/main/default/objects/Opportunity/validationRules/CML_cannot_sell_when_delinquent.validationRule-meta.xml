<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>CML_cannot_sell_when_delinquent</fullName>
    <active>false</active>
    <errorConditionFormula>AND(
 Account.MM_Delinquent__c, 
 Account.Account_Size_Segment__c = &apos;AE 1&apos;
)</errorConditionFormula>
    <errorMessage>This customer is delinquent. You cannot sell to a delinquent customer.</errorMessage>
</ValidationRule>
