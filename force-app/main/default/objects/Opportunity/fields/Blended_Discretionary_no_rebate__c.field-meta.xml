<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Blended_Discretionary_no_rebate__c</fullName>
    <formula>IF (List_Price__c &gt; 0, 

IF( ISPICKVAL(Selected_Payment_Type__c,&quot;Direct Annual&quot;), 

((List_Price_Less_Default_Discounts__c - (Amount)) / List_Price__c)-0.1, 


((List_Price_Less_Default_Discounts__c - (Amount)) / List_Price__c) 

) 

, 0)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Blended Discretionary (no rebate)</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <type>Percent</type>
</CustomField>
