<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Subsidy_Amount__c</fullName>
    <description>Subsidy amount from rebate opportunity., updated for GTMS-27234</description>
    <formula>IF(
    SBQQ__PrimaryQuote__r.New_DC_architecture__c,
    SBQQ__PrimaryQuote__r.Subsidy_Amount__c * -1,
    Subsidy_Opportunity__r.Rebate_Amount__c
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Subsidy amount from rebate opportunity.</inlineHelpText>
    <label>Sum of Subsidy</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <type>Currency</type>
</CustomField>
