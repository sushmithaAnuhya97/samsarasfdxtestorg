<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Opportunity_Age__c</fullName>
    <description>Used to calculate the opportunity to be piped back into Gong</description>
    <externalId>false</externalId>
    <formula>IF(IsClosed,  (CloseDate -  DATEVALUE(CreatedDate))  ,( TODAY() - DATEVALUE(CreatedDate)))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Opportunity Age</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
