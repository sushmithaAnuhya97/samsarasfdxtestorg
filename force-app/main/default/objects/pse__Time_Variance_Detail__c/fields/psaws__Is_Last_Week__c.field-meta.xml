<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>psaws__Is_Last_Week__c</fullName>
    <deprecated>false</deprecated>
    <description>The formula field checks whether the related Time Variance Calculations Date Time is of current week or not.</description>
    <formula>ISPICKVAL(pse__Time_Variance_Calculation__r.pse__Date_Type__c, &quot;Last Week&quot;)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Is Last Week</label>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>
