<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Hour_of_transfer_acceptance__c</fullName>
    <description>Stamps the hour the transfer was accepted</description>
    <externalId>false</externalId>
    <formula>MOD(FLOOR(HOUR( TIMEVALUE( ADR_Accepted_Date__c ) ))-6+23,24)</formula>
    <inlineHelpText>Stamps the hour the transfer was accepted</inlineHelpText>
    <label>Hour of transfer acceptance</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
