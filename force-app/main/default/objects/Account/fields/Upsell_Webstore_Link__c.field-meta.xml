<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Upsell_Webstore_Link__c</fullName>
    <description>Samsara existing customer webstore link</description>
    <externalId>false</externalId>
    <formula>IF(
   AND(
        Lifetime_Purchases__c &gt; 0, 
        ISPICKVAL(CurrencyIsoCode, &apos;USD&apos;),
        CONTAINS(Owner.UserRole.Name, &apos;AE1&apos;)
       ), 
   &apos;https://samsara.com/webstore?src=sfdc&apos;, &apos;&apos;)</formula>
    <label>Customer Upsell Webstore Link</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
