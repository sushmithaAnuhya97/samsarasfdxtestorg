<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Cycle_Time__c</fullName>
    <description>New account cycle time, defined as first revenue opportunity creation date to first purchase date for the account.</description>
    <externalId>false</externalId>
    <formula>IF(AND(NOT(ISBLANK(First_Purchase_Date__c)),NOT(ISBLANK(First_Revenue_Oppty_Creation_Date__c))),First_Purchase_Date__c-DATEVALUE(First_Revenue_Oppty_Creation_Date__c),Null)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Cycle Time - 1st Oppty to 1st Purchase</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
