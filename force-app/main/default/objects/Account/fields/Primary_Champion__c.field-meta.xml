<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Primary_Champion__c</fullName>
    <deleteConstraint>SetNull</deleteConstraint>
    <description>Primary contact within an Account record. Used by AEs to more quickly prospect, &quot;bookmark&quot; certain contacts, and is also helpful when accounts are transferred.</description>
    <inlineHelpText>Populate with the main point of contact (champion) for more efficient prospecting/account management.</inlineHelpText>
    <label>Primary Champion</label>
    <lookupFilter>
        <active>true</active>
        <filterItems>
            <field>Contact.Account.Id</field>
            <operation>equals</operation>
            <valueField>$Source.Id</valueField>
        </filterItems>
        <isOptional>false</isOptional>
    </lookupFilter>
    <referenceTo>Contact</referenceTo>
    <relationshipLabel>Accounts (Primary Contact)</relationshipLabel>
    <relationshipName>Accounts3</relationshipName>
    <required>false</required>
    <trackFeedHistory>false</trackFeedHistory>
    <trackHistory>false</trackHistory>
    <type>Lookup</type>
</CustomField>
