<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Upsold_Account__c</fullName>
    <description>Account has a second purchase greater than $100</description>
    <formula>AND( Last_Purchase_Date__c &gt; First_Purchase_Date__c ,  Lifetime_Purchases__c -First_Purchase_Value__c &gt;100)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Upsold Account</label>
    <trackHistory>false</trackHistory>
    <type>Checkbox</type>
</CustomField>
