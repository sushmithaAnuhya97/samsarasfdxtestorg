<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Free_Trial_Activity_Status__c</fullName>
    <description>Retrieves the status of the trial: 1) It not been activated yet. 2) It was activated but there has been no activity in the past 7 days. 3) It was activated and there has been activity in the past 7 days.</description>
    <externalId>false</externalId>
    <formula>IF(ISNULL(Free_Trial_First_Device_Claim_Datetime__c), &quot;Not activated (unclaimed)&quot;, IF(ISNULL(Free_Trial_Last_Activity_Datetime__c), &quot;Inactive (last 7 days)&quot;, IF(TODAY()-DATEVALUE(Free_Trial_Last_Activity_Datetime__c)&gt;7,&quot;Inactive (last 7 days)&quot;, &quot;Active (last 7 days)&quot; )))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Free Trial Activity Status</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
