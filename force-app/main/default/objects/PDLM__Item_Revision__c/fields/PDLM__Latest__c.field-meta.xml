<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>PDLM__Latest__c</fullName>
    <deprecated>false</deprecated>
    <description>Calculate LATEST or SUPERSEDED</description>
    <externalId>false</externalId>
    <formula>IF( PDLM__Master_Item__r.PDLM__Latest_Released_Revision__c  = Id, &quot;Latest&quot;,
  if( NOT( PDLM__Released__c ),&quot;Draft&quot;,
    &quot;Superseded&quot;))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Revision State</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
