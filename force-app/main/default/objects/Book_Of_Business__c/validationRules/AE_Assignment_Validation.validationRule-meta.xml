<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>AE_Assignment_Validation</fullName>
    <active>false</active>
    <errorConditionFormula>AND (
				AE_Validation_Check__c,
				OR(
								ISCHANGED(AE__c),
								ISNEW()
								)
)</errorConditionFormula>
    <errorMessage>AE Cannot be assigned, as they have already been assigned to an existing Account Book,</errorMessage>
</ValidationRule>
