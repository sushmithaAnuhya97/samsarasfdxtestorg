<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>PDLM__Baseline_Due_Date_Before_Start_Date</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
NOT(ISNULL(PDLM__DueDate__c)),
NOT(ISNULL(PDLM__Baseline_Start_Date__c)),
PDLM__DueDate__c &lt; PDLM__Baseline_Start_Date__c)</errorConditionFormula>
    <errorDisplayField>PDLM__DueDate__c</errorDisplayField>
    <errorMessage>The baseline finish date needs to be a later date than the start date.</errorMessage>
</ValidationRule>
