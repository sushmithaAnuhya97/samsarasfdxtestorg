<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Interest_Rate__c</fullName>
    <description>Interest Rate (cost of debt)</description>
    <formula>IF((Financing_Term__c = 0.00 || ISBLANK(Financing_Term__c) || ISBLANK(Approved_Amount__c ) || Customer_Monthly_Payment__c = 0.00 || Approved_Amount__c = 0),VALUE(&quot;&quot;), 

(EXP((1/Financing_Term__c*12)*ln((Customer_Monthly_Payment__c *Financing_Term__c)/Approved_Amount__c)))-1)</formula>
    <label>Interest Rate</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Percent</type>
</CustomField>
