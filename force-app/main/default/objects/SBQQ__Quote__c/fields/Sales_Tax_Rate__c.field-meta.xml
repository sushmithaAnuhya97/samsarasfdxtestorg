<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Sales_Tax_Rate__c</fullName>
    <formula>IF(AND(NOT(ISBLANK(Shipping_Address_NEW__c)),
SBQQ__NetAmount__c &gt; 0), Total_Sales_Tax__c / (SBQQ__NetAmount__c+ IF(ISPICKVAL(Is_Installation_Reimbursable__c, &apos;No&apos;),Total_Estimated_Installation_Costs__c,0)), 99999)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Sales Tax Rate</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Percent</type>
</CustomField>
