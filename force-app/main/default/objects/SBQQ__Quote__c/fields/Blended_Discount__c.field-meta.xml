<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Blended_Discount__c</fullName>
    <formula>IF(
 List_Amount_Rebate__c &gt; 0, 
 ((List_Amount_Rebate__c - (Net_Amount_Rebate__c - (Buyout_Amount__c +  Subsidy_Amount__c )))/ List_Amount_Rebate__c), 0)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Blended Discount</label>
    <precision>18</precision>
    <required>false</required>
    <scale>1</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Percent</type>
</CustomField>
