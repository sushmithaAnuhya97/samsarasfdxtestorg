<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Original_Approved_Discount__c</fullName>
    <formula>MIN(
	IF(
		NOT(ISBLANK(SBQQ__Opportunity__r.Approved_Discount__c)),
		SBQQ__Opportunity__r.Approved_Discount__c, 
		IF(
			SBQQ__Opportunity__r.Blended_Discount__c=0,NULL,
			(SBQQ__Opportunity__r.Blended_Discount__c+0.001)
		)
	),0.401
)

