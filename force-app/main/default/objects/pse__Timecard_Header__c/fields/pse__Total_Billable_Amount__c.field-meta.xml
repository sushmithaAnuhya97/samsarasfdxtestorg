<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>pse__Total_Billable_Amount__c</fullName>
    <deprecated>false</deprecated>
    <formula>IF(pse__Billable__c,IF(pse__Daily_Bill_Rate__c,(pse__Total_Days_Worked__c * pse__Bill_Rate__c),(pse__Total_Hours__c * pse__Bill_Rate__c)), 0.00)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Total Billable Amount</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Currency</type>
</CustomField>
