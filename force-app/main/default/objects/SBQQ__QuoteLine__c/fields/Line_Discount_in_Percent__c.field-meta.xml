<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Line_Discount_in_Percent__c</fullName>
    <formula>NULLVALUE(SBQQ__Discount__c, 
          IF( 
             SBQQ__RegularPrice__c &gt; 0, 
             SBQQ__AdditionalDiscountAmount__c/SBQQ__RegularPrice__c, 
             0
          ) 
)</formula>
    <label>Line Discount in Percent</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Percent</type>
</CustomField>
