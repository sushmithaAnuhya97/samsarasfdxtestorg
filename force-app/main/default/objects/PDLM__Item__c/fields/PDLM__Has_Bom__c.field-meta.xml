<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>PDLM__Has_Bom__c</fullName>
    <deprecated>false</deprecated>
    <description>Checks the latest released revision to see if it has a bom.  If there is no latest released, falls back to latest pending.</description>
    <formula>if (PDLM__Latest_Released_Revision__c != null, PDLM__Latest_Released_Revision__r.PDLM__Has_Bom__c, PDLM__Latest_Revision__r.PDLM__Has_Bom__c)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Has BOM</label>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>
