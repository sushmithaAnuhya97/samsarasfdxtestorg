<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>NetsuiteBot_Cannot_change_Contact_Email</fullName>
    <active>false</active>
    <description>Netsuite Bot cannot update Contact Email</description>
    <errorConditionFormula>ISCHANGED(Email) &amp;&amp; $User.FirstName = &quot;Netsuite&quot; &amp;&amp; $User.LastName= &quot;Bot&quot;</errorConditionFormula>
    <errorMessage>Netsuite Bot cannot change the Contact Email</errorMessage>
</ValidationRule>
