<?xml version="1.0" encoding="UTF-8"?>
<ListView xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Tracked_Contacts</fullName>
    <booleanFilter>1 AND 2 AND 3 AND (4 OR 5)</booleanFilter>
    <columns>CONTACT.FIRST_NAME</columns>
    <columns>CONTACT.LAST_NAME</columns>
    <columns>ACCOUNT.NAME</columns>
    <columns>CONTACT.PHONE1</columns>
    <columns>X18_digit_Contact_ID__c</columns>
    <columns>LinkedIn_URL__c</columns>
    <columns>CONTACT.ADDRESS2_CITY</columns>
    <columns>CONTACT.ADDRESS2_STATE_CODE</columns>
    <columns>CONTACT.ADDRESS2_STREET</columns>
    <columns>CloudingoAgent__MAS__c</columns>
    <columns>CONTACT.ADDRESS2_ZIP</columns>
    <columns>Job_Tracking_Last_Reviewed_Date__c</columns>
    <filterScope>Everything</filterScope>
    <filters>
        <field>LinkedIn_URL__c</field>
        <operation>notEqual</operation>
    </filters>
    <filters>
        <field>Account.Customer_ARR_Netsuite__c</field>
        <operation>greaterThan</operation>
        <value>0</value>
    </filters>
    <filters>
        <field>ACCOUNT.ADDRESS1_COUNTRY_CODE</field>
        <operation>equals</operation>
        <value>US</value>
    </filters>
    <filters>
        <field>Status__c</field>
        <operation>notEqual</operation>
        <value>Faulty Lead</value>
    </filters>
    <filters>
        <field>Inbound_Reason_Faulty__c</field>
        <operation>equals</operation>
        <value>Inactive - No Longer There</value>
    </filters>
    <label>Job Tracking - Tracked Contacts</label>
    <sharedTo>
        <role>Management</role>
    </sharedTo>
</ListView>
