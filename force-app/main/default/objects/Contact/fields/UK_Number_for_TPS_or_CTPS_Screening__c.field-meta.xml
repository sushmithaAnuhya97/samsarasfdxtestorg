<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>UK_Number_for_TPS_or_CTPS_Screening__c</fullName>
    <description>Used to determine if PhoneTools TPS &amp; CTPS Screening is needed - for GDPR compliance.</description>
    <externalId>false</externalId>
    <formula>IF(OR(MailingCountry=&quot;United Kingdom&quot;,ISBLANK(MailingCountry)), 1, 0)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>UK Number for TPS or CTPS Screening</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
