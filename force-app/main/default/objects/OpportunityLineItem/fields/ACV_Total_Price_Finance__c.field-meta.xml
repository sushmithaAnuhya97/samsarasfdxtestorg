<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ACV_Total_Price_Finance__c</fullName>
    <formula>IF(Opportunity.License_Term__c = 0, 0, (TotalPrice / Opportunity.License_Term__c) * 12 )</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>ACV Total Price Finance</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <type>Currency</type>
</CustomField>
