<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Suggested_Line_Price__c</fullName>
    <description>Displays Suggested Sales Price * License Term for new licenses and Suggested Sales Price for hardware.</description>
    <formula>IF(LEFT(Product_Code_Copy__c,3)=&quot;LIC&quot;,Suggested_Sales_Price__c*License_Term_Copy__c,Suggested_Sales_Price__c)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Standard Discounted Price</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <type>Currency</type>
</CustomField>
