<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Valid_Quota__c</fullName>
    <description>Field to check if the quota is valid for the given date.</description>
    <formula>AND(TODAY() &gt;= Quota_Period_Start_Date__c , TODAY() &lt;= Quota_Period_End_Date__c)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Valid Quota</label>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>
