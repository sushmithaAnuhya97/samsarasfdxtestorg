<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Ramp_Month__c</fullName>
    <description>Number of months a rep has been in their role at the start of the quota period</description>
    <externalId>false</externalId>
    <formula>IF( ( Fiscal_Period_Start_Date__c  -   Adj_Role_Start_Date_v2__c  ) / 365 * 12 &lt; 1, 

((Fiscal_Period_Start_Date__c  -  Adj_Role_Start_Date_v2__c) / 365 * 12 ) + 1,

(CEILING(Fiscal_Period_Start_Date__c - Adj_Role_Start_Date_v2__c) / 365 * 12) + 1

)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Number of months a rep has been in their role at the start of the quota period</inlineHelpText>
    <label>Ramp Month</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
