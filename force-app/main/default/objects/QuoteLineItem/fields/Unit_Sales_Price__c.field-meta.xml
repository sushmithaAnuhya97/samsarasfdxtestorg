<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Unit_Sales_Price__c</fullName>
    <description>*New System* price of unit with discount</description>
    <formula>IF(UnitPrice&gt;Unit_List_Price__c+1, UnitPrice, Unit_List_Price__c*(1.00-Discount))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Unit Sales Price</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <type>Currency</type>
</CustomField>
