        <fields>DNBConnect__D_B_Name_Match_Score__c</fields>
        <fields>DUNS_Number__c</fields>
        <fields>Id</fields>
        <fields>NAICS_Code__c</fields>
        <fields>Name</fields>
        <fields>Phone</fields>
        <fields>Website</fields>
        <includeSessionId>false</includeSessionId>
        <integrationUser>d&amp;bconnect@samsara.com</integrationUser>
        <name>ConnectAccountTrigger</name>
        <protected>false</protected>
        <useDeadLetterQueue>false</useDeadLetterQueue>
    </outboundMessages>
    <rules>
        <fullName>AE1 Sales Notification</fullName>
        <actions>
            <name>AE1_Sales_Alert</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <formula>Account_Size_Segment__c  = &apos;AE1&apos; &amp;&amp;  Customer_ARR__c &gt; 5000 &amp;&amp; ((Today() - First_Purchase_Date__c) &lt; 365) &amp;&amp; ((TODAY()-DATEVALUE(Delinquent_As_Of__c )) &lt; 30)</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>Account Churn Risk Reason Email Send</fullName>
        <actions>
            <name>CSM_Account_At_Risk_for_Churn</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>Account.Churn_Risk_Reason__c</field>
            <operation>notEqual</operation>
        </criteriaItems>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>Account%3A Segment</fullName>
        <actions>
            <name>Field_Segment_Text_stamped</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <description>This text field brings in the Segment value from the formula field: Segment (by Current owner) field on the account.</description>
        <formula>OR( 
(ISCHANGED( OwnerId )), 
ISBLANK( Segment_Text_stamped__c ) 
)</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Owner Change</fullName>
        <actions>
            <name>Email_Alert_for_Previous_and_Current_Owner_of_an_account</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <formula>AND(Current_Owner_Email__c != null, Previous_Owner_Email__c != null, ISCHANGED(OwnerId),  OR(AND( NOT(Owner.RR_Is_Pool_User__c),  NOT(PRIORVALUE(Is_Owner_Pool_User__c))), AND(Owner.RR_Is_Pool_User__c, NOT(PRIORVALUE(Is_Owner_Pool_User__c)), $User.FirstName=&quot;Low Engagement&quot; )))</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Stamp CSM Manager Email</fullName>
        <actions>
            <name>Stamp_CSM_Manager_Email</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <formula>AND(ISCHANGED( CSM__c ),NOT(ISBLANK(CSM__c)))</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Update Global Geography</fullName>
        <actions>
            <name>Geo_Update_on_State</name>
            <type>FieldUpdate</type>
        </actions>
        <active>false</active>
        <description>Update Global Geography field based on states / province and billing country</description>
        <formula>OR( AND(ISCHANGED( BillingCountry ), BillingCountry != &quot;United States&quot;),   AND(ISCHANGED(BillingState), BillingState != NULL ),ISBLANK( Global_Geography__c )  )</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Upsell email to AE</fullName>
        <actions>
            <name>CSM_Account_With_Upsell_Opportunity</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>Account.Potential_Upsell_Opportunity__c</field>
            <operation>equals</operation>
            <value>True</value>
        </criteriaItems>
        <description>Sends email to AE based on CSM&apos;s assessment of a potential upsell opportunity</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
</Workflow>
