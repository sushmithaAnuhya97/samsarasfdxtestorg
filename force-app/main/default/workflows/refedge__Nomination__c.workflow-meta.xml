    <rules>
        <fullName>refedge__Nominate Account</fullName>
        <actions>
            <name>refedge__POR_Nomination_for_Account</name>
            <type>Alert</type>
        </actions>
        <active>false</active>
        <formula>refedge__Email_Enabled__c = true &amp;&amp; refedge__Account__c &lt;&gt; null &amp;&amp; refedge__Contact__c = null &amp;&amp;  ISPICKVAL( refedge__Status__c , &apos;Not Started&apos;)</formula>
        <triggerType>onCreateOnly</triggerType>
    </rules>
    <rules>
