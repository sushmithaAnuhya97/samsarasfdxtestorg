<?xml version="1.0" encoding="UTF-8"?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>62.0</apiVersion>
    <areMetricsLoggedToDataCloud>false</areMetricsLoggedToDataCloud>
    <decisions>
        <description>GTMS-25300: To check if Account is converted as partner</description>
        <name>Account_Partner_Conversion_Check</name>
        <label>Account Partner Conversion Check</label>
        <locationX>1370</locationX>
        <locationY>458</locationY>
        <defaultConnector>
            <targetReference>Partner_Account_Message_Screen</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Not Converted To Partner</defaultConnectorLabel>
        <rules>
            <name>Partner_Account</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Get_Contact.Account.IsPartner</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Transacting_Partner_Account_Check</targetReference>
            </connector>
            <label>Partner Account</label>
        </rules>
    </decisions>
    <decisions>
        <description>GTMS-25300: Created as part of partner registration phase 2 project CR changes</description>
        <name>PR_record_check</name>
        <label>PR record check</label>
        <locationX>776</locationX>
        <locationY>350</locationY>
        <defaultConnector>
            <targetReference>Account_Partner_Conversion_Check</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Existing_User</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Get_Partner_User.Id</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>User_Activation_Check</targetReference>
            </connector>
            <label>Existing User</label>
        </rules>
    </decisions>
    <decisions>
        <description>GTMS-25300: Created as part of partner application phase 2 to check if the Account is a transacting partner</description>
        <name>Transacting_Partner_Account_Check</name>
        <label>Transacting Partner Account Check</label>
        <locationX>1106</locationX>
        <locationY>566</locationY>
        <defaultConnector>
            <targetReference>Non_Transacting_Partner_Screen</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Not a Transacting Partner</defaultConnectorLabel>
        <rules>
            <name>Transacting_Partner</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Get_Contact.Account.Partner_Status__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Transacting Partner</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Create_User_Creation_Event</targetReference>
            </connector>
            <label>Transacting Partner</label>
        </rules>
    </decisions>
    <decisions>
        <description>GTMS-25300: Created as part of partner application phase 2 to check if existing user is active or not</description>
        <name>User_Activation_Check</name>
        <label>User Activation Check</label>
        <locationX>182</locationX>
        <locationY>458</locationY>
        <defaultConnector>
            <targetReference>Activate_User</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Active_User</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Get_Partner_User.IsActive</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Active_User_Message_Screen</targetReference>
            </connector>
            <label>Active User</label>
        </rules>
    </decisions>
