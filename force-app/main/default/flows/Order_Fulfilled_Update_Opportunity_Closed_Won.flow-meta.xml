            <name>Is_FT_Order</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.RecordType.DeveloperName</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Free_Trial_Order</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Netsuite_Id_And_Mulesoft_Sync_Error_is_Changed</targetReference>
            </connector>
            <label>Is FT Order?</label>
        </rules>
    </decisions>
