    <decisions>
        <name>Check_if_the_Net_Suite</name>
        <label>Check if the Net Suite/Mulesoft flag has been update</label>
        <locationX>1177</locationX>
        <locationY>966</locationY>
        <defaultConnector>
            <targetReference>On_update_of_Fulfilment_Date</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>check_if_Flag_is_update_to_true</name>
            <conditionLogic>((1 AND 2) OR (3 AND 4)) AND (5)</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Push_to_Mulesoft__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Push_to_Mulesoft__c</leftValueReference>
                <operator>IsChanged</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Push_To_NetSuite__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Push_To_NetSuite__c</leftValueReference>
                <operator>IsChanged</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record__Prior.Sync_in_Progress__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Assign_Sync_F_ag</targetReference>
            </connector>
            <label>check if Flag is update to true</label>
        </rules>
    </decisions>
