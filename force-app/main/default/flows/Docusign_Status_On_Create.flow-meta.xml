    <formulas>
        <description>to store bill to contact full Name - GTMS-16637</description>
        <name>Bill_To_Contact_Name</name>
        <dataType>String</dataType>
        <expression>{!$Record.dsfs__Opportunity__r.SBQQ__PrimaryQuote__r.Bill_to_Contact__r.FirstName}+&apos; &apos;+{!$Record.dsfs__Opportunity__r.SBQQ__PrimaryQuote__r.Bill_to_Contact__r.LastName}</expression>
    </formulas>
    <formulas>
        <description>GTMS-16637: Created as part of DCA Project</description>
        <name>BIllingEmail</name>
        <dataType>String</dataType>
        <expression>IF(
NOT(ISBLANK({!$Record.dsfs__Opportunity__r.SBQQ__PrimaryQuote__r.Bill_to_Account__r.Billing_Email__c})),
{!$Record.dsfs__Opportunity__r.SBQQ__PrimaryQuote__r.Bill_to_Account__r.Billing_Email__c},
{!$Record.dsfs__Opportunity__r.SBQQ__PrimaryQuote__r.Bill_to_Contact__r.Email}
)</expression>
    </formulas>
    <formulas>
        <description>GTMS-20366</description>
        <name>OVAEntryCheck</name>
        <dataType>Boolean</dataType>
        <expression>AND(
	NOT(
	{!$Record.dsfs__Opportunity__r.Custom_Schedule__c} = true
	|| NOT(ISPICKVAL({!$Record.dsfs__Opportunity__r.Type}, &apos;Revenue Opportunity&apos;))
	|| CONTAINS({!$Record.dsfs__Opportunity__r.Owner_Role_Copy__c}, &apos;SLED&apos;)
	|| CONTAINS({!$Record.dsfs__Opportunity__r.Owner_Manager_Role_Copy__c}, &apos;SLED&apos;)
	|| NOT(({!$Record.dsfs__Opportunity__r.Finance_Segment__c} = &apos;Enterprise&apos; &amp;&amp; {!$Record.dsfs__Opportunity__r.Is_Webstore_Upsell_Opportunity__c} = true) || {!$Record.dsfs__Opportunity__r.Finance_Segment__c} == &apos;Mid Market&apos;)
	|| {!$Record.dsfs__Opportunity__r.Payment_Type__c} = &apos;Reseller&apos;
	|| {!$Record.dsfs__Opportunity__r.Owner_Segment__c} = &apos;AE1&apos;
	|| {!$Record.dsfs__Opportunity__r.Payment_Type__c} = &apos;Finance Partner&apos;
	|| {!$Record.dsfs__Opportunity__r.SBQQ__Renewal__c} = true
	|| CONTAINS({!$Record.dsfs__Opportunity__r.Name},&apos;INVENTORYSPLIT&apos;)
	)
)</expression>
    </formulas>
    <formulas>
        <description>GTMS-18053: to created bill contact name value from queried quote record.</description>
