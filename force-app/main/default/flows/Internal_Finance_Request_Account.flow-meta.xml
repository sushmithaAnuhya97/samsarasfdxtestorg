    <formulas>
        <name>ApplyForFinancingDeconstructed</name>
        <dataType>String</dataType>
        <expression>IF(
   CONTAINS({!ApplyForFinancing} , &quot;;3rd Party Finance&quot;),
    SUBSTITUTE(SUBSTITUTE({!ApplyForFinancing} , &quot;;&quot;, &quot; and &quot;), &quot; and 3rd Party Finance&quot;, &quot;&quot;),
    SUBSTITUTE({!ApplyForFinancing} , &quot;;&quot;, &quot; and &quot;)
)</expression>
    </formulas>
    <formulas>
        <name>Doubleofmaxcreditlimit</name>
        <dataType>Currency</dataType>
        <expression>{!thisAccount.Credit_Limit__c} * 2</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>ExtractAccountId</name>
        <dataType>String</dataType>
        <expression>IF({!recordId} ==NULL ,{!inputAccountId},{!Get_Quote.SBQQ__Account__c} )</expression>
    </formulas>
    <formulas>
        <name>Halfofmaxcreditlimit</name>
        <dataType>Currency</dataType>
        <expression>{!thisAccount.Credit_Limit__c} * 0.5</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <description>Used to determine which region an account is being billed</description>
        <name>NorthAmericanAccount</name>
        <dataType>Boolean</dataType>
        <expression>NOT(CONTAINS({!$Label.Account_Global_Geography}, {!thisAccount.Global_Geography__c}))</expression>
