ISCHANGED([Opportunity].StageName), 
ISPICKVAL([Opportunity].Type, &apos;Revenue Opportunity&apos; ), 
OR(AND([Opportunity].ACV_Bookings_SOT__c&gt;833,[Opportunity].Express_Flag__c=1), AND([Opportunity].ACV_Bookings_SOT__c&gt;6250,[Opportunity].Express_Flag__c=0), AND([Opportunity].ACV_Bookings_SOT__c&gt;250,[Opportunity].Express_Flag__c=0,
