    <decisions>
        <description>Renewable ACV (Opportunity.Renewable_ACV__c) &lt; $10K and Customer ARR (Account.Customer_ARR__c ) &lt; $25K
Finance partner (Opportunity.Finance_Partner__c  = False) &amp; Payment type (Opportunity. Payment_Type__c &lt;&gt; &apos;Upfront&apos;/&apos;Reseller&apos;</description>
        <name>Escalation_Criteria_1</name>
        <label>Escalation Criteria 1</label>
        <locationX>384</locationX>
        <locationY>617</locationY>
        <defaultConnectorLabel>Criteria Not Met</defaultConnectorLabel>
        <rules>
            <name>Criteria_Met</name>
            <conditionLogic>1 AND 2 AND 3 AND (4 OR 5) AND 6</conditionLogic>
            <conditions>
                <leftValueReference>Get_Opportunity_Details.Renewable_ACV__c</leftValueReference>
                <operator>LessThan</operator>
                <rightValue>
                    <numberValue>10000.0</numberValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Get_Account_Details.Customer_ARR__c</leftValueReference>
                <operator>LessThan</operator>
                <rightValue>
                    <numberValue>25000.0</numberValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Get_Opportunity_Details.Finance_Partner__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Get_Opportunity_Details.Payment_Type__c</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue>Upfront</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Get_Opportunity_Details.Payment_Type__c</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue>Reseller</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Get_Contract_Details.Auto_Renewal__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Update_Contract</targetReference>
            </connector>
            <label>Criteria Met</label>
        </rules>
    </decisions>
