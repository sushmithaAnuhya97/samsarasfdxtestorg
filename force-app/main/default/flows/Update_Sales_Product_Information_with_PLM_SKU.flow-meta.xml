    <decisions>
        <name>Check_if_Category_is_SKU</name>
        <label>Check if Category is SKU</label>
        <locationX>380</locationX>
        <locationY>276</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Yes</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.PDLM__Master_Item__r.PDLM__Category__r.Name</leftValueReference>
                <operator>Contains</operator>
                <rightValue>
                    <stringValue>SKU</stringValue>
                </rightValue>
