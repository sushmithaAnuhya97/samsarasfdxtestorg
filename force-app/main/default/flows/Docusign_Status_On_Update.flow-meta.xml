        <expression>ISNEW()</expression>
    </formulas>
    <formulas>
        <description>to check if triggered records satisfies entry cirteria</description>
        <name>OVAEntryCriteria</name>
        <dataType>Boolean</dataType>
        <expression>AND(
NOT(
{!$Record.dsfs__Opportunity__r.Custom_Schedule__c} = true
|| NOT(ISPICKVAL({!$Record.dsfs__Opportunity__r.Type}, &apos;Revenue Opportunity&apos;))
|| CONTAINS({!$Record.dsfs__Opportunity__r.Owner_Role_Copy__c}, &apos;SLED&apos;)
|| CONTAINS({!$Record.dsfs__Opportunity__r.Owner_Manager_Role_Copy__c}, &apos;SLED&apos;)
|| NOT(({!$Record.dsfs__Opportunity__r.Finance_Segment__c} = &apos;Enterprise&apos; &amp;&amp; {!$Record.dsfs__Opportunity__r.Is_Webstore_Upsell_Opportunity__c} = true) || {!$Record.dsfs__Opportunity__r.Finance_Segment__c} == &apos;Mid Market&apos;)
|| {!$Record.dsfs__Opportunity__r.Payment_Type__c} = &apos;Reseller&apos;
|| {!$Record.dsfs__Opportunity__r.Owner_Segment__c} = &apos;AE1&apos;
|| {!$Record.dsfs__Opportunity__r.Payment_Type__c} = &apos;Finance Partner&apos;
|| {!$Record.dsfs__Opportunity__r.SBQQ__Renewal__c} = true
|| CONTAINS({!$Record.dsfs__Opportunity__r.Name},&apos;INVENTORYSPLIT&apos;)
)
)</expression>
    </formulas>
