    <formulas>
        <description>Opp name + &quot; - Buyout&quot;</description>
        <name>newOppName</name>
        <dataType>String</dataType>
        <expression>{!Get_source_opportunity.Name}+&quot; - Subsidy&quot;</expression>
    </formulas>
    <formulas>
        <name>rebateCurrencyValue</name>
        <dataType>String</dataType>
        <expression>TEXT({!subsidyOpportunity.CurrencyIsoCode})+&apos; &apos;+ TEXT({!subsidyOpportunity.Rebate_Amount__c})</expression>
    </formulas>
    <formulas>
        <name>subsidyRebateValue</name>
        <dataType>Currency</dataType>
        <expression>({!Get_Primary_Quote_from_Source_Opp.Subsidy_Amount__c} * -1)</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>toastMessageText</name>
        <dataType>String</dataType>
        <expression>SUBSTITUTE({!$Label.Buyout_Subsidy_Opp_Creation_Toast_Message}, &apos;{1}&apos;, {!rebateCurrencyValue})</expression>
    </formulas>
