        <rules>
            <name>Industrial</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>fUserRole</leftValueReference>
                <operator>Contains</operator>
                <rightValue>
                    <stringValue>IND</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Industrial_Demo_Subflow</targetReference>
            </connector>
            <label>Industrial</label>
        </rules>
        <rules>
            <name>Fleet_Enterprise_or_Sites</name>
            <conditionLogic>(1 AND 2) OR 3 OR 4</conditionLogic>
            <conditions>
                <leftValueReference>fUserRole</leftValueReference>
                <operator>Contains</operator>
                <rightValue>
                    <stringValue>ENT AE</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>fUserRole</leftValueReference>
                <operator>Contains</operator>
                <rightValue>
                    <stringValue>Fleet</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>fUserRole</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Management</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>fUserRole</leftValueReference>
                <operator>Contains</operator>
                <rightValue>
                    <stringValue>Sites</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Fleet_Enterprise_Subflow</targetReference>
            </connector>
            <label>Fleet Enterprise or Sites</label>
        </rules>
    </decisions>
