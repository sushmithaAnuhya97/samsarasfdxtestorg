    <formulas>
        <name>Error_Automation_Notes</name>
        <dataType>String</dataType>
        <expression>IF(
  ISBLANK({!ErrorMessage}),
  {!$Label.Order_Automation_Default_Error_Message},
  IF(
    CONTAINS({!ErrorMessage}, &quot;FIELD_CUSTOM_VALIDATION_EXCEPTION&quot;),
    MID(
      {!ErrorMessage},
      FIND(&quot;FIELD_CUSTOM_VALIDATION_EXCEPTION&quot;, {!ErrorMessage}) + LEN(&quot;FIELD_CUSTOM_VALIDATION_EXCEPTION&quot;),
      255
    ),
    IF(
      CONTAINS({!ErrorMessage}, &quot;FIELD_FILTER_VALIDATION_EXCEPTION&quot;),
      MID(
        {!ErrorMessage},
        FIND(&quot;FIELD_FILTER_VALIDATION_EXCEPTION&quot;, {!ErrorMessage}) + LEN(&quot;FIELD_FILTER_VALIDATION_EXCEPTION&quot;),
        255
      ),
      {!$Label.Order_Automation_Default_Error_Message}
    )
  )
)</expression>
    </formulas>
