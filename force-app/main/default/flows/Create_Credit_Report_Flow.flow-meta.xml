    <environments>Default</environments>
    <formulas>
        <name>CreditRatingValue</name>
        <dataType>String</dataType>
        <expression>IF(OR({!thisCredit.Credit_Scorecard__c}&gt;7.0,{!oldCreditReport.Credit_Scorecard__c}&gt;7.0), &apos;green&apos;, &apos;yellow&apos;)</expression>
    </formulas>
    <formulas>
        <name>CreditScoreDisclaimer</name>
        <dataType>String</dataType>
        <expression>IF({!thisCredit.Credit_Scorecard__c}&gt;7.0, {!thisCredit.Credit_Rating__c},&apos;&apos; )</expression>
    </formulas>
    <formulas>
        <name>CRtimeLapse</name>
        <dataType>Date</dataType>
        <expression>{!oldCreditReport.CreatedDate}</expression>
    </formulas>
    <formulas>
        <name>NorthAmericanAccount</name>
        <dataType>Boolean</dataType>
        <expression>NOT(CONTAINS({!$Label.Account_Global_Geography}, {!accountVariable.Global_Geography__c}))</expression>
    </formulas>
    <formulas>
        <name>TodayDate</name>
        <dataType>Date</dataType>
        <expression>Today()</expression>
    </formulas>
    <formulas>
        <description>To check whether credit report is executed from current date-1 or not</description>
        <name>YesterdayDate</name>
        <dataType>Date</dataType>
        <expression>TODAY()-1</expression>
    </formulas>
