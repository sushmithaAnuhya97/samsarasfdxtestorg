    <description>Identify if lead is a Kickfire Lead</description>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>OR (AND(CONTAINS([CampaignMember].Campaign.Name, &apos;Kickfire&apos;), 
                       [CampaignMember].Account__c.CreatedDate &gt; NOW() -  ((0.25*60) / 1440), 
                       [CampaignMember].Account__c.CreatedDate &lt; NOW() + ((0.25*60) / 1440),
                       
                      CONTAINS([CampaignMember].Account__c.First_Touch_Campaign__c.Name , &apos;Kickfire&apos;)),
AND (CONTAINS([CampaignMember].Campaign.Name, &apos;Kickfire&apos;), 
                   [CampaignMember].Account__c.Previous_Owner_Name__c  = &apos;SFDC Pool&apos;,
                   [CampaignMember].Account__c.Account_Assignment_Date__c  &lt; NOW() + ((0.25*60) / 1440),
                   [CampaignMember].Account__c.Account_Assignment_Date__c  &gt; NOW() - ((0.25*60) / 1440)
           
         )
)</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_5</name>
        <dataType>Boolean</dataType>
        <expression>OR (AND(CONTAINS({!myVariable_current.Campaign.Name}, &apos;Kickfire&apos;), 
                       {!myVariable_current.Account__r.CreatedDate} &gt; NOW() -  ((0.25*60) / 1440), 
                       {!myVariable_current.Account__r.CreatedDate} &lt; NOW() + ((0.25*60) / 1440),
                       
                      CONTAINS({!myVariable_current.Account__r.First_Touch_Campaign__r.Name} , &apos;Kickfire&apos;)),
AND (CONTAINS({!myVariable_current.Campaign.Name}, &apos;Kickfire&apos;), 
                   {!myVariable_current.Account__r.Previous_Owner_Name__c}  = &apos;SFDC Pool&apos;,
                   {!myVariable_current.Account__r.Account_Assignment_Date__c}  &lt; NOW() + ((0.25*60) / 1440),
                   {!myVariable_current.Account__r.Account_Assignment_Date__c}  &gt; NOW() - ((0.25*60) / 1440)
           
         )
)</expression>
    </formulas>
