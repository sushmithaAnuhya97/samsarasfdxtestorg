<?xml version="1.0" encoding="UTF-8"?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>59.0</apiVersion>
    <areMetricsLoggedToDataCloud>false</areMetricsLoggedToDataCloud>
    <decisions>
        <name>Check_if_it_has_atleast_one_FPQ</name>
        <label>Check if it has atleast one FPQ</label>
        <locationX>704</locationX>
        <locationY>492</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Does_CPQ_has_FPQ_associated</name>
            <conditionLogic>(1 OR 2 OR 6) AND 3 AND 4 AND 5</conditionLogic>
            <conditions>
                <leftValueReference>$Record.License_Term__c</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <elementReference>Get_FPQ_Records.Term_Length__c</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Balance_Due__c</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <elementReference>Get_FPQ_Records.Balance_Due__c</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>IsAEprofiles</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Does_3PF_require_approval__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Get_FPQ_Records.Financing_Decision__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Approved</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>ShippingAddressConcatenated</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <elementReference>Get_FPQ_Records.Customer_Shipping_Address__c</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Enable_Does_Approval_Required</targetReference>
            </connector>
            <label>Does CPQ has FPQ associated</label>
        </rules>
        <rules>
            <name>check_clear_banner</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Does_3PF_require_approval__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.License_Term__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <elementReference>Get_FPQ_Records.Term_Length__c</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Balance_Due__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <elementReference>Get_FPQ_Records.Balance_Due__c</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>ShippingAddressConcatenated</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <elementReference>Get_FPQ_Records.Customer_Shipping_Address__c</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Get_FPQ_Records.Financing_Decision__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Resubmit For Review</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Update_FPQ_Approve</targetReference>
            </connector>
            <label>check clear banner</label>
        </rules>
    </decisions>
    <decisions>
        <name>Skip_Flows</name>
        <label>Skip Flows?</label>
        <locationX>440</locationX>
        <locationY>276</locationY>
        <defaultConnector>
            <targetReference>Get_FPQ_Records</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>True</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Setup.Global_Automation_Settings__c.Skip_Flow_Automation__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <label>True</label>
        </rules>
    </decisions>
    <environments>Default</environments>
    <formulas>
        <name>IsAEprofiles</name>
        <dataType>Boolean</dataType>
        <expression>NOT(CONTAINS({!$User.ProfileId},&apos;00e4p000000VhSb&apos;))</expression>
    </formulas>
    <formulas>
        <name>profileid</name>
