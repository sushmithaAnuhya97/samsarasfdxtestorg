    </formulas>
    <formulas>
        <name>ShippingAddressConcatenated</name>
        <dataType>String</dataType>
        <expression>{!$Record.Shipping_Address_NEW__r.Shipping_FirstName__c} &amp;&quot; &quot;&amp;{!$Record.Shipping_Address_NEW__r.Shipping_LastName__c}&amp;&quot;&lt;br&gt;&quot;&amp;
{!$Record.Shipping_Address_NEW__r.Shipping_Address_Line_1__c}&amp;&quot;&lt;br&gt;&quot;&amp;
{!$Record.Shipping_Address_NEW__r.Shipping_Address_Line_2__c}&amp;&quot;&lt;br&gt;&quot;&amp;
{!$Record.Shipping_Address_NEW__r.Shipping_City__c}&amp;&quot;, &quot;&amp;
TEXT( {!$Record.Shipping_Address_NEW__r.Shipping_State__c})&amp;&quot; &quot;&amp;
{!$Record.Shipping_Address_NEW__r.Shipping_Zip_Code__c}&amp;&quot;&lt;br&gt;&quot;&amp;
TEXT({!$Record.Shipping_Address_NEW__r.Shipping_Country__c})</expression>
    </formulas>
