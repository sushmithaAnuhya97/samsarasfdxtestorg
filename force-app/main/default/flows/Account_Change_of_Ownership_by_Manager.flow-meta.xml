    <decisions>
        <name>IsOpptyOpen</name>
        <label>IsOpptyOpen</label>
        <locationX>886</locationX>
        <locationY>1322</locationY>
        <defaultConnector>
            <targetReference>ChangeOpptyLoop</targetReference>
