    <description>This flow is used to set the &quot;Channel Partner Discount&quot; field on quote
GTMS-12023- Removed Free trial Approval Status Update and added it to Apex</description>
    <environments>Default</environments>
    <formulas>
        <name>firstPurchase</name>
        <dataType>Boolean</dataType>
        <expression>{!$Record.SBQQ__Account__r.First_Purchase_Date__c} = {!$Record.SBQQ__Opportunity2__r.CloseDate} || ISBLANK({!$Record.SBQQ__Account__r.First_Purchase_Date__c})</expression>
    </formulas>
    <formulas>
        <name>ownerSegment</name>
        <dataType>String</dataType>
        <expression>IF(

CONTAINS({!$Record.SBQQ__Opportunity2__r.Owner_Role_Copy__c},&quot;Small Fleets&quot;),
&quot;Small Business&quot;,
IF(
OR(
CONTAINS({!$Record.SBQQ__Opportunity2__r.Owner_Role_Copy__c},&quot;AE1&quot;),
CONTAINS({!$Record.SBQQ__Opportunity2__r.Owner_Role_Copy__c},&quot;SMB&quot;)
),
&quot;AE1&quot;,
IF(
OR(
CONTAINS({!$Record.SBQQ__Opportunity2__r.Owner_Role_Copy__c},&quot;AE2&quot;),
CONTAINS({!$Record.SBQQ__Opportunity2__r.Owner_Role_Copy__c},&quot;MM&quot;)
),
&quot;AE2&quot;,
IF(
CONTAINS({!$Record.SBQQ__Opportunity2__r.Owner_Role_Copy__c},&quot;AE3&quot;),
&quot;AE3&quot;,
IF(
OR(
CONTAINS({!$Record.SBQQ__Opportunity2__r.Owner_Role_Copy__c},&quot;Major&quot;),
CONTAINS({!$Record.SBQQ__Opportunity2__r.Owner_Role_Copy__c},&quot;Enterprise&quot;),
CONTAINS({!$Record.SBQQ__Opportunity2__r.Owner_Role_Copy__c},&quot;ENT&quot;),
CONTAINS({!$Record.SBQQ__Opportunity2__r.Owner_Role_Copy__c},&quot;Field&quot;)
),
&quot;Enterprise&quot;,
&quot;Other&quot;
)
)
)
)
)</expression>
    </formulas>
    <formulas>
        <name>sledRole</name>
        <dataType>Boolean</dataType>
        <expression>OR(
CONTAINS({!ownerSegment}, &quot;SLED&quot;),
CONTAINS({!$Record.Owner:User.Profile.Name}, &quot;Sales Ops&quot;)
)</expression>
    </formulas>
