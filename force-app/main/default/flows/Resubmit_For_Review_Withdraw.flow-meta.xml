    <decisions>
        <description>GTMS-16419: created as part of 3PF phase 2 project</description>
        <name>Entry_Check_of_FPQ</name>
        <label>Entry Check of FPQ</label>
        <locationX>540</locationX>
        <locationY>271</locationY>
        <defaultConnector>
            <targetReference>Entry_Error_Screeen</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>FPQ_Entry_Check</name>
            <conditionLogic>(1 OR 2 OR 3 OR 5 OR 6 OR 7) AND 4</conditionLogic>
            <conditions>
                <leftValueReference>Get_FPQ_Record.Financing_Decision__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Approved</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Get_FPQ_Record.Financing_Decision__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Declined</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Get_FPQ_Record.Financing_Decision__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Need More Info</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Get_FPQ_Record.Opportunity__r.Probability</leftValueReference>
                <operator>LessThanOrEqualTo</operator>
                <rightValue>
                    <numberValue>90.0</numberValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Get_FPQ_Record.Financing_Decision__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Resubmit For Review</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Get_FPQ_Record.Financing_Decision__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Pending - Under Partner Review</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Get_FPQ_Record.Financing_Decision__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Pending</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Update_FPQ_Record</targetReference>
            </connector>
            <label>FPQ Entry Check</label>
        </rules>
    </decisions>
