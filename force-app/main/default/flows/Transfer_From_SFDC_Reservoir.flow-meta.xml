    <formulas>
        <name>OrganizationSizeCheckCML</name>
        <dataType>Boolean</dataType>
        <expression>OR(
    ISBLANK(TEXT({!varAccountRecord.Organization_Size__c})), 
    ISPICKVAL({!varAccountRecord.Organization_Size__c}, &quot;1 - 99&quot;),
    ISPICKVAL({!varAccountRecord.Organization_Size__c}, &quot;100 - 499&quot;),
    ISPICKVAL({!varAccountRecord.Organization_Size__c}, &quot;500 - 999&quot;)
)</expression>
    </formulas>
    <formulas>
        <name>OrganizationSizeCheckENT</name>
        <dataType>Boolean</dataType>
        <expression>OR(
    ISBLANK(TEXT({!varAccountRecord.Organization_Size__c})),
    ISPICKVAL({!varAccountRecord.Organization_Size__c}, &quot;500 - 999&quot;),
    ISPICKVAL({!varAccountRecord.Organization_Size__c}, &quot;1000 - 4999&quot;),
    ISPICKVAL({!varAccountRecord.Organization_Size__c}, &quot;5000+&quot;)
)</expression>
    </formulas>
    <formulas>
        <name>OrganizationSizeCheckMM</name>
        <dataType>Boolean</dataType>
        <expression>OR(
    ISBLANK(TEXT({!varAccountRecord.Organization_Size__c})), 
    ISPICKVAL({!varAccountRecord.Organization_Size__c}, &quot;1 - 99&quot;),
    ISPICKVAL({!varAccountRecord.Organization_Size__c}, &quot;100 - 499&quot;),
    ISPICKVAL({!varAccountRecord.Organization_Size__c}, &quot;500 - 999&quot;),
    ISPICKVAL({!varAccountRecord.Organization_Size__c}, &quot;1000 - 4999&quot;)
)</expression>
    </formulas>
    <formulas>
        <name>PoolCreditFormula</name>
        <dataType>Number</dataType>
        <expression>IF({!varUserRecord.Pool_Credits__c}&gt;0,({!varUserRecord.Pool_Credits__c} -1),0)</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <name>SalesOpsNotesFormula</name>
        <dataType>String</dataType>
        <expression>IF(ISBLANK({!varAccountRecord.Sales_Ops_Notes__c}), 
    &apos;#cmlreservoirtransferfy25&apos;, 
    {!varAccountRecord.Sales_Ops_Notes__c} &amp; &apos;, &apos; &amp;
 &apos;#cmlreservoirtransferfy25&apos;)</expression>
    </formulas>
    <formulas>
        <name>TransitioningGeography</name>
        <dataType>String</dataType>
        <expression>TEXT({!varUserRecord.Transitioning_Geography__c})</expression>
    </formulas>
