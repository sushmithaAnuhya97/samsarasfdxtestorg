public without sharing class R2ShipCWPlatformEventHelper {

    public static void r2ShipCWPlatormEventDispatch(List<R2Ship_CW_Opportunity__e> r2ShipCWPE) {
        Set<Id> opportunityIds = new Set<Id>();

        for(R2Ship_CW_Opportunity__e pe: r2ShipCWPE){
            if(pe.OpportunityId__c != null && !pe.Is_Closed_Won__c && pe.Revenue_Opportunity__c && pe.Send_Virtual_Bell__c){
                opportunityIds.add(pe.OpportunityId__c);
            }
        }

        if(opportunityIds.size() > 0){
            virtualBellMatch(opportunityIds);
        }
    }
    
    //* 8/20/2020 Ron (GTMS-1354) virtualBellMatch (enable scalable method of determining business Units by ACV/TCV)
    @Future
    public static void virtualBellMatch(Set<Id> opportunityIds){
        Map<Id, Opportunity> oppMap = new Map<Id, Opportunity>([SELECT  Id, 
                                                                        Amount, 
                                                                        ACV_Bookings_SOT__c, 
                                                                        Virtual_Bell_Thank_You_2__c,
                                                                        Virtual_Bell_Win_Story__c,
                                                                        OwnerId,
                                                                        Owner.FirstName,
                                                                        Owner.Name,
                                                                        Owner_Role_Copy__c,
                                                                        Owner.Team_Email_Alias__c,
                                                                        Account.Name, 
                                                                        Shipping_Contact__c, 
                                                                        Shipping_Address_NEW__r.Shipping_Contact__c
                                                                FROM Opportunity
                                                                WHERE Id IN : opportunityIds]);

        Map<Id, Tiered_Virtual_Bell__mdt> vbMetadata = new Map<Id, Tiered_Virtual_Bell__mdt>([  SELECT  Label, 
                                                                                                        Email_Template_Id__c, 
                                                                                                        Lower_Bound__c, 
                                                                                                        Upper_Bound__c, 
                                                                                                        User_Role__c,
                                                                                                        To_Email_1__c,
                                                                                                        To_Email_2__c, 
                                                                                                        Global_Thank_You__c 
                                                                                                FROM Tiered_Virtual_Bell__mdt]);
        List<Opportunity> updateOpps = new List<Opportunity>();

        OrgWideEmailAddress dealAlertsEmail = [SELECT Id, Address FROM OrgWideEmailAddress WHERE Address = 'deal-alerts@samsara.com' LIMIT 1];
        OrgWideEmailAddress sfdcRobotEmail = [SELECT Id, Address FROM OrgWideEmailAddress WHERE Address = 'sfdc-robot@samsara.com' LIMIT 1];  
        List<Messaging.SendEmailResult> emailResults;
        String acvUnits = System.Label.Business_Unit_ACV;
        String tcvUnits = System.Label.Business_Unit_TCV;

        for(Opportunity opp: oppMap.values()){
            Tiered_Virtual_Bell__mdt selectedTVB;

            List<String> listToEmailAddresses = new List<String>();
            String teamEmail = opp.Owner.Team_Email_Alias__c != null? opp.Owner.Team_Email_Alias__c: null;
            	
            String businessUnit0, businessUnit1, businessUnitAcv, businessUnitTcv;
            List<String> noOfWords = new List<String>();
                if(String.isNotBlank(opp.Owner_Role_Copy__c)){
                    noOfWords = opp.Owner_Role_Copy__c.split('\\s+');
                	businessUnit0 = opp.Owner_Role_Copy__c.split(' ')[0];}
                if(noOfWords.size() > 1){
                    businessUnit1 = opp.Owner_Role_Copy__c.split(' ')[1];
                    businessUnitAcv = acvUnits.contains(businessUnit1)? (businessUnit1) : (businessUnit0);
					businessUnitTcv = tcvUnits.contains(businessUnit1)? (businessUnit1) : (businessUnit0);
            	}else{
                     businessUnitAcv =  businessUnit0;
                     businessUnitTcv =  businessUnit0;
                }
            
            for(Tiered_Virtual_Bell__mdt tvb: vbMetadata.values()){
                //String businessUnit = opp.Owner_Role_Copy__c.split(' ')[0];
                if(String.isNotBlank(businessUnitAcv) && acvUnits.contains(businessUnitAcv)){
                    selectedTVB = getTVB(opp, tvb, 'acv', businessUnitAcv);
                    if(selectedTVB != null){
                        break;
                    }
                }

                if(String.isNotBlank(businessUnitTcv) && tcvUnits.contains(businessUnitTcv)){
                    selectedTVB = getTVB(opp, tvb, 'tcv', businessUnitTcv);
                    if(selectedTVB != null){
                        break;
                    }
                }
            }

            if(selectedTVB != null){
                OrgWideEmailAddress fromEmail = selectedTVB.Label.contains('Team')? sfdcRobotEmail: dealAlertsEmail;

                String stringToEmailAddresses = selectedTVB.To_Email_1__c != null? selectedTVB.To_Email_1__c: null;
                if(stringToEmailAddresses != null){
                    listToEmailAddresses = stringToEmailAddresses.deleteWhitespace().split(',');
                }

                if(teamEmail != null && selectedTVB.Label.contains('Team')){
                    listToEmailAddresses.add(teamEmail);
                }

                if(listToEmailAddresses.size() > 0){
                    try{
                        emailResults = sendEmail(opp, selectedTVB.Email_Template_Id__c, listToEmailAddresses, fromEmail);
                    }
                    catch(Exception e){
                        System.debug('There was an exception when trying to send Virtual Bell Email OpportunityId: ' + opp.Id + '\n\nException can be found here: '+ e);
                    }
                }
                else{
                    System.debug('List of Email Addresses to send Virtual Bell Email are null. \n\nPlease review Team Email Alias and Virtual Bell Custom Metadata for the '+ 'selectedTVB.Label'+' scenario on OpportunityId: '+ opp.Id);
                }
                
                if(emailResults != null && !emailResults.isEmpty() && emailResults[0].success){
                    Opportunity oppUpdate = new Opportunity(Id = opp.Id, Send_Virtual_Bell__c = false);
                    updateOpps.add(oppUpdate);
                }
            }
        }
		update updateOpps;
    }

    //* August 20th, 2020 Ron Saavedra (GTMS-1354) virtualBellMatch (enable scalable method of determining business Units by ACV/TCV)
    public static Tiered_Virtual_Bell__mdt getTVB(Opportunity opp, Tiered_Virtual_Bell__mdt tvb, String type, String businessUnit){
		Decimal amountType = type == 'acv'? opp.ACV_Bookings_SOT__c: opp.Amount;
        Boolean globalThank = ((opp.Virtual_Bell_Thank_You_2__c != null|| opp.Virtual_Bell_Win_Story__c != null) && tvb.Label.contains('Global')) ? true: false;
        tvb.User_Role__c = tvb.User_Role__c.trim();

        List<String> multiRoles = tvb.User_Role__c.contains(',') ? tvb.User_Role__c.split(','): null;
        Boolean multiRoleMatch = false;

        if(multiRoles != null){
            for(String mR : multiRoles) {
                mR = mR.trim();
                if(opp.Owner_Role_Copy__c.contains(mR)){
                    multiRoleMatch = true;
                    break;
                }
            }
        }
        Boolean meetsBoundReq = (amountType >= tvb.Lower_Bound__c && amountType <= tvb.Upper_Bound__c)? true:false;
        if(tvb.Upper_Bound__c == null){
            if(tvb.Label.contains(businessUnit) && (opp.Owner_Role_Copy__c.contains(tvb.User_Role__c) || multiRoleMatch)
                && amountType > tvb.Lower_Bound__c && globalThank == tvb.Global_Thank_You__c){
                return tvb;
            }
        }
        else{
            if(tvb.Label.contains(businessUnit) && (opp.Owner_Role_Copy__c.contains(tvb.User_Role__c) || multiRoleMatch) 
                && amountType >= tvb.Lower_Bound__c && amountType <= tvb.Upper_Bound__c && globalThank == tvb.Global_Thank_You__c){
                return tvb;
            }
        }

        return null;
    }

    //TODO - consolidate this into a service class to allow for calls across the codebase. RN we're copying and pasting all over the place
    private static Messaging.SendEmailResult[] sendEmail(Opportunity opp, Id emailTemplateId, List<String> toEmailAddresses, OrgWideEmailAddress fromEmail){

        Id shippingContactId = opp.Shipping_Contact__c == null ? opp.Shipping_Address_NEW__r.Shipping_Contact__c: opp.Shipping_Contact__c;

        Messaging.SingleEmailMessage email = new Messaging.SingleEmailMessage();
        email.setTemplateId(emailTemplateId);
        email.setTargetObjectId(shippingContactId);
        email.setTreatTargetObjectAsRecipient(false);
        email.setToAddresses(toEmailAddresses);
        email.setWhatId(opp.Id);
        email.saveAsActivity = false;

        Messaging.SingleEmailMessage mail = new Messaging.SingleEmailMessage();
        if (fromEmail.Address != null) {
            email.setOrgWideEmailAddressId(fromEmail.Id);
        }

        List<Messaging.SendEmailResult> resultSend = Messaging.sendEmail(new List<Messaging.SingleEmailMessage>{ email });
        return resultSend;
    }
